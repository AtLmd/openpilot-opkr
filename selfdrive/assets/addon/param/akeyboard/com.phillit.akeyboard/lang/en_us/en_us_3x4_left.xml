            longTab="char"
            mode="all"
            position="0"
            previewable="false"
            shortTab="text"
            texts="*Toggle*."
            width="25%" />
        <key
            codes="a"
            ends="top"
            keys="c:2;t:a;t:b;t:c"
            label="abc"
            labelSize="19"
            longTab="minikbd"
            position="1"
            previewable="false"
            rightOf="#prev"
            shiftCodes="A"
            shortTab="char"
            width="25%" />
        <key
